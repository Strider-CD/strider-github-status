{
  "name": "strider-github-status",
  "version": "2.1.0",
  "description": "Add build status indicators to pull requests and commits in github",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/Strider-CD/strider-github-status.git"
  },
  "keywords": [
    "strider",
    "github",
    "pull-request",
    "status",
    "commit"
  ],
  "engines": {
    "node": ">=4.2"
  },
  "author": "Jared Forsyth <jared@jaredforsyth.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Strider-CD/strider-github-status/issues"
  },
  "homepage": "https://github.com/Strider-CD/strider-github-status",
  "dependencies": {
    "github": "~9.0.0",
    "debug": "~2.6.1"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "scripts": {
    "lint": "eslint *.js lib",
    "test-only": "mocha -- -R spec",
    "pretest": "npm run lint",
    "test": "istanbul cover ./node_modules/mocha/bin/_mocha -- -R spec",
    "posttest": "istanbul check-coverage",
    "prepublish": "npm test && npm prune"
  },
  "strider": {
    "type": "job",
    "id": "github-status",
    "title": "Github Status",
    "webapp": "webapp.js",
    "worker": "worker.js",
    "icon": "icon.png",
    "config": {
      "controller": "GithubStatusCtrl"
    }
  },
  "devDependencies": {
    "eslint": "^3.1.1",
    "expect.js": "~0.3.1",
    "istanbul": "~0.4.5",
    "jshint": "~2.9.4",
    "mocha": "~3.2.0"
  }
}
